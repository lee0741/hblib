{
   "name": "湖北省图书馆豆瓣插件",
   "version": "1.1",
   "manifest_version": 2,
   "description": "在豆瓣读书页面显示书籍在湖北省图书馆的索引信息",
   "homepage_url": "http://lee0741.github.io/hblib",
   "update_url": "https://raw.github.com/lee0741/hblib/master/update.xml"
   "content_scripts": [{
      "js": ["jquery.js", "inject.js"],
      "matches": ["http://book.douban.com/subject/*"]
   }],
   "permissions": ["http://hbtinterlib.library.hb.cn/opac/*"]
}
